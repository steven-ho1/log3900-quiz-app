{
    "name": "client-web",
    "version": "1.0.0",
    "main": "electron/main.js",
    "scripts": {
        "start": "ng serve --open",
        "build": "ng build",
        "build:dev": "ng build -c development",
        "electron": "npm run build && electron electron/main.js",
        "electron:dev": "npm run build:dev && electron electron/main.js",
        "package": "npm run build && electron-builder",
        "test": "ng test",
        "coverage": "ng test --code-coverage --source-map=true",
        "lint": "ng lint --format stylish",
        "lint:fix": "npm run lint -- --fix"
    },
    "private": true,
    "dependencies": {
        "@angular/animations": "16.1.8",
        "@angular/cdk": "16.1.7",
        "@angular/common": "16.1.8",
        "@angular/compiler": "16.1.8",
        "@angular/core": "16.1.8",
        "@angular/forms": "16.1.8",
        "@angular/material": "16.1.7",
        "@angular/platform-browser": "16.1.8",
        "@angular/platform-browser-dynamic": "16.1.8",
        "@angular/router": "16.1.8",
        "@ngx-translate/core": "^16.0.3",
        "@ngx-translate/http-loader": "^16.0.0",
        "ci": "^2.3.0",
        "file-saver": "^2.0.5",
        "file-saver-es": "^2.0.5",
        "http-status-codes": "^2.3.0",
        "lodash-es": "^4.17.21",
        "ngx-image-compress": "^18.1.5",
        "qrcode": "^1.5.4",
        "rxjs": "7.8.1",
        "socket.io-client": "4.7.2",
        "tslib": "2.4.0",
        "zone.js": "0.13.1"
    },
    "devDependencies": {
        "@angular-devkit/build-angular": "^16.2.5",
        "@angular-eslint/builder": "16.1.0",
        "@angular-eslint/eslint-plugin": "16.1.0",
        "@angular-eslint/eslint-plugin-template": "16.1.0",
        "@angular-eslint/schematics": "16.1.0",
        "@angular-eslint/template-parser": "16.1.0",
        "@angular/cli": "16.1.8",
        "@angular/compiler-cli": "16.1.8",
        "@types/file-saver": "^2.0.5",
        "@types/file-saver-es": "^2.0.3",
        "@types/jasmine": "4.3.5",
        "@types/lodash": "^4.14.199",
        "@types/lodash-es": "^4.17.12",
        "@types/node": "20.4.8",
        "@types/qrcode": "^1.5.5",
        "@typescript-eslint/eslint-plugin": "^6.2.1",
        "@typescript-eslint/parser": "^6.2.1",
        "electron": "^34.0.0",
        "electron-builder": "^25.1.8",
        "eslint": "8.46.0",
        "eslint-config-prettier": "9.0.0",
        "eslint-import-resolver-typescript": "3.5.5",
        "eslint-plugin-deprecation": "1.5.0",
        "eslint-plugin-import": "2.28.0",
        "eslint-plugin-jsdoc": "46.4.6",
        "eslint-plugin-prefer-arrow": "1.2.3",
        "eslint-plugin-prettier": "5.0.0",
        "eslint-plugin-unicorn": "48.0.1",
        "jasmine-core": "5.1.0",
        "karma": "6.4.2",
        "karma-chrome-launcher": "3.2.0",
        "karma-coverage": "2.2.1",
        "karma-jasmine": "5.1.0",
        "karma-jasmine-html-reporter": "2.1.0",
        "prettier": "3.0.1",
        "rimraf": "^6.0.1",
        "typescript": "4.9.5"
    },
    "build": {
        "appId": "com.log3900.trivia-theater",
        "productName": "Trivia Theater",
        "files": [
            "dist/client-web/**/*",
            "electron/**/*"
        ],
        "win": {
            "target": [
                "nsis"
            ],
            "icon": "dist/client-web/favicon.ico"
        }
    }
}
